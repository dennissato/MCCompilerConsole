{
    "exe": "MCCompilerConsole.exe",
    "mode": [
        {
            "Release": [
                {
                    "extension": [
                        ".mcs",
                        ".macs"
                    ],
                    "argumentorder": [
                        "ExecutionDirectory",
                        "SourceFileName",
                        "LogFile",
                        "Language",
                        "Release",
                        "CompileMCASDir",
                        "CompileMCBINDir",
                        "CompileMCDir",
                        "AssembleMCBINDir",
                        "AssembleMCDir"
                    ]
                }
            ],
            "Debug": [
                {
                    "extension": [
                        ".mcs",
                        ".macs"
                    ],
                    "argumentorder": [
                        "ExecutionDirectory",
                        "SourceFileName",
                        "LogFile",
                        "Language",
                        "Debug",
                        "CompileMCASDir",
                        "CompileMCBINDir",
                        "CompileMCDir",
                        "AssembleMCBINDir",
                        "AssembleMCDir"
                    ]
                }
            ],
            "Disassemble": [
                {
                    "extension": [
                        ".mc"
                    ],
                    "argumentorder": [
                        "ExecutionDirectory",
                        "SourceFileName",
                        "LogFile",
                        "Language"
                    ]
                }
            ]
        }
    ],
    "arguments": [
        {
            "Release": "r",
            "Debug": "c",
            "CompileMCASDir": "\\mcas",
            "CompileMCBINDir": "\\mcbin",
            "CompileMCDir": "\\mc",
            "AssembleMCBINDir": "\\mcbin",
            "AssembleMCDir": "\\mc"
        }
    ]
}